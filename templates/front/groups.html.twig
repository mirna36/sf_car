{% extends 'front/base.html.twig' %}

{% block body %}
    {{ parent() }}
    <main class="container">
        <h1 class="nosVoiture">Nos Goupes</h1>
     
       <table class="table">
			<thead class="table-dark">
				<tr>
					<th scope="col">Nom</th>
					<th scope="col">Pays</th>
					
				</tr>
			</thead>
			<tbody>
				<tr>
					{% for group in groups %}
						<td>{{group.name}}</td>
						<td>{{group.countrie}}</td>
						
					</tr>
				{% endfor %}
			</tbody>
		</table>
		
	
        
        
    </main>
    {% block javascripts %}{% endblock %}      

{% endblock %}